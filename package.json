{
  "name": "api.ts.joeladams.dev",
  "version": "1.0.0",
  "description": "Backend API built using Typescript",
  "main": "index.js",
  "scripts": {
    "prestart": "npx prettier --write . --loglevel warn && echo \"$(tput setaf 6) Ran Prettier formatter$(tput setaf 7)\" && npm run database-build",
    "start": "nodemon --files index.ts | pino-pretty",
    "build": "tsc",
    "database-build": "ts-node knex/createDB.ts && npx knex migrate:latest && npx knex seed:run",
    "database-teardown": "npx knex migrate:rollback",
    "database-update": "npx knex migrate:latest",
    "kill-port": "sh kill-port.sh"
  },
  "keywords": [],
  "author": "Joel Adams",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.4",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "pg": "^8.7.1",
    "pino": "^7.6.1",
    "serve-favicon": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/express-pino-logger": "^4.0.3",
    "@types/pino": "^7.0.5",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "express-pino-logger": "^7.0.0",
    "nodemon": "^2.0.15",
    "npx": "^10.2.2",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.5.1"
  }
}
